{
  "compilerOptions": {
     "rootDir": "./src",
     "outDir": "./dist",
    "module": "commonjs",
    "target": "es6",
    "lib": ["es6"],

    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
  },
  "include": ["src/**/*" ],
}
